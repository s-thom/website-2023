---
import LocalImage from "./Images/LocalImage.astro";
import { GooseButton } from "./react/GooseButton/index.tsx";
---

<GooseButton client:visible>
  <LocalImage
    id="footer-goose"
    resourcePath="goose.png"
    class="goose-image"
    alt="Goose"
    widths={[48, 96]}
    sizes="48px"
    loading="lazy"
  />
</GooseButton>

<style>
  .goose-image {
    width: 48px;
    height: 48px;
  }
</style>
